{
  "model": "gemini-2.5-flash",
  "mcpServers": {
    "Playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--headless",
        "--no-sandbox",
        "--isolated"
      ]
    },
    "DBHub": {
      "command": "npx",
      "args": [
        "-y",
        "@bytebase/dbhub",
        "--transport",
        "stdio",
        "--dsn",
        "mariadb://${DB1_USER}:${DB1_PASS}@${DB1_ENDPOINT}:${DB1_PORT}/${DB1_DATABASE_NAME}"
      ]
    }
  }
}